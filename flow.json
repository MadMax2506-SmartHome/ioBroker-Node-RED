        "name": "hs to rgb",
        "func": "const { rgb } = msg;\n\nmsg.payload = {\n    red: rgb[0],\n    green: rgb[1],\n    blue: rgb[2]\n}\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
